def has_user_management_permission(role_name: str):
    """Check if the given role has user management permissions for the given tenant.
     Args:
        role_name: Name of the role to check permissions for

    Returns: Boolean indicating if the role has user management permissions.
    """

    accepted_roles = [
        "tenant_owner"
    ]  # Creates a list of roles with user management permissions. Easily extendable.

    if role_name in accepted_roles:
        return True
    else:
        return False